  "query": "\n        UPDATE teams\n        SET name          = COALESCE($2, name),\n            supervisor_id = CASE WHEN $3 THEN $4 ELSE supervisor_id END,\n            is_active     = COALESCE($5, is_active)\n        WHERE id = $1 AND org_id = $6\n        RETURNING id, org_id, name, supervisor_id, is_active, created_at\n        ",
